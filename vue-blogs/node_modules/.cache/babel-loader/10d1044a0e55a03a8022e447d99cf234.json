;AAIA,MAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EALA,CAMA;;AACA,UAAA,SAAA,GAAA,EAAA,EAAA;AACA,aAAA,WAAA,GAAA,IAAA;AACA,OAFA,MAEA;AACA,aAAA,WAAA,GAAA,KAAA;AACA;AACA,KAbA;AAcA,IAAA,MAdA,oBAcA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA,KAAA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAlBA;AAmBA,IAAA,aAnBA,2BAmBA;AAAA;;AACA,MAAA,cAAA,GACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAHA,EAIA,KAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OANA;AAOA;AA3BA;AApBA,CAAA","sourcesContent":["<template>\n  <div\n    id=\"layout-header\"\n    :class=\"{ fixed: fixed, hidden: hidden }\"\n    @click.stop=\"mobileShow = false\"\n  >\n    <div class=\"site-logo\">\n      <router-link to=\"/\">\n        <p class=\"site-name\">ShaoHangBlog</p>\n      </router-link>\n    </div>\n    <div class=\"menus-btn\" @click.stop=\"mobileShow = !mobileShow\">Menus</div>\n    <div\n      v-if=\"!$store.state.isLogin\"\n      class=\"site-menus\"\n      :class=\"{ mobileShow: mobileShow }\"\n      @click.stop=\"mobileShow = !mobileShow\"\n    >\n      <!-- <div class=\"menu-item header-search\"><header-search /></div> -->\n      <div class=\"menu-item\"><router-link to=\"/\">首页</router-link></div>\n      <div class=\"menu-item hasChild\">\n        <a href=\"#\">文章</a>\n        <div class=\"childMenu\" v-if=\"category.length\">\n          <div class=\"sub-menu\" v-for=\"item in category\" :key=\"item.title\">\n            <router-link :to=\"`/category/${item.title}`\">{{\n              item.title\n            }}</router-link>\n          </div>\n        </div>\n      </div>\n      <div class=\"menu-item\"><router-link to=\"/about\">关于</router-link></div>\n      <div class=\"menu-item\"><router-link to=\"/login\">登录</router-link></div>\n      <div class=\"menu-item\">\n        <router-link to=\"/register\">注册</router-link>\n      </div>\n    </div>\n    <div\n      v-else\n      class=\"site-menus\"\n      :class=\"{ mobileShow: mobileShow }\"\n      @click.stop=\"mobileShow = !mobileShow\"\n    >\n      <!-- <div class=\"menu-item header-search\"><header-search /></div> -->\n      <div class=\"menu-item\"><router-link to=\"/\">首页</router-link></div>\n      <div class=\"menu-item hasChild\">\n        <a href=\"#\">文章</a>\n        <div class=\"childMenu\" v-if=\"category.length\">\n          <div class=\"sub-menu\" v-for=\"item in category\" :key=\"item.title\">\n            <router-link :to=\"`/category/${item.title}`\">{{\n              item.title\n            }}</router-link>\n          </div>\n        </div>\n      </div>\n      <div class=\"menu-item\"><router-link to=\"/about\">关于</router-link></div>\n      <div v-if=\"$store.state.isAdmin\" class=\"menu-item\">\n        <router-link to=\"/admin\">后台管理</router-link>\n      </div>\n      <div class=\"menu-item\" @click=\"logout\">注销</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport HeaderSearch from \"@/components/header-search\";\nimport { fetchCategory } from \"../../api\";\nexport default {\n  name: \"layout-header\",\n  components: { HeaderSearch },\n  data() {\n    return {\n      navBarFixed: false,\n      fixed: false,\n      hidden: false,\n      category: [],\n      mobileShow: false,\n      isAdmin:false,\n    };\n  },\n  created(){\n    console.log('是否管理员',this.$store.state.isAdmin);\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.watchScroll);\n    this.fetchCategory();\n  },\n  methods: {\n    watchScroll() {\n      var scrollTop =\n        window.pageYOffset ||\n        document.documentElement.scrollTop ||\n        document.body.scrollTop;\n        console.log(scrollTop)\n      //  当滚动超过 90 时，实现吸顶效果\n      if (scrollTop > 90) {\n        this.navBarFixed = true;\n      } else {\n        this.navBarFixed = false;\n      }\n    },\n    logout() {\n      this.$store.commit(\"setIsLogin\", false);\n      localStorage.removeItem(\"token\");\n      this.$router.push({ name: \"login\" });\n    },\n    fetchCategory() {\n      fetchCategory()\n        .then((res) => {\n          this.category = res.data;\n        })\n        .catch((err) => {\n          console.log(err);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"less\">\n#layout-header {\n  position: fixed;\n  top: 0;\n  z-index: 9;\n  width: 100%;\n  height: 80px;\n  padding: 0 80px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  transition: 0.3s all ease;\n  -webkit-transition: 0.3s all ease;\n  -moz-transition: 0.3s all linear;\n  -o-transition: 0.3s all ease;\n  -ms-transition: 0.3s all ease;\n  &.hidden {\n    top: -100px;\n  }\n  &.fixed {\n    // background-color: #ffffff;\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n  }\n}\n\n.site-logo {\n  text-align: center;\n\n  img {\n    width: 60px;\n    height: 60px;\n  }\n\n  p.site-name {\n    font-size: 15px;\n    font-weight: bold;\n    position: relative;\n    top: -10px;\n  }\n}\n.menus-btn {\n  display: none;\n  visibility: hidden;\n}\n.site-menus {\n  display: flex;\n  align-items: center;\n\n  .menu-item {\n    min-width: 60px;\n    height: 50px;\n    line-height: 50px;\n    text-align: center;\n    position: relative;\n    a {\n      padding: 12px 10px;\n      color: #000000;\n      font-weight: 500;\n      font-size: 16px;\n      &:hover {\n        color: #ff6d6d;\n      }\n    }\n    &:not(:last-child) {\n      margin-right: 15px;\n    }\n    &.hasChild:hover .childMenu {\n      opacity: 1;\n      visibility: visible;\n      transform: translateY(-5px);\n    }\n  }\n  .childMenu {\n    width: 130px;\n    background-color: #fdfdfd;\n    border-radius: 3px;\n    border: 1px solid #ddd;\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n    position: absolute;\n    top: 55px;\n    z-index: 2;\n    opacity: 0;\n    visibility: hidden;\n    transition: 0.7s all ease;\n    -webkit-transition: 0.6s all ease;\n    -moz-transition: 0.6s all linear;\n    -o-transition: 0.6s all ease;\n    -ms-transition: 0.6s all ease;\n    &:before,\n    &:after {\n      content: \"\";\n      position: absolute;\n      width: 0;\n      height: 0;\n      border-left: 6px solid transparent;\n      border-right: 6px solid transparent;\n      border-bottom: 8px solid white;\n      top: -8px;\n      left: 20px;\n    }\n    &:before {\n      top: -9px;\n      border-bottom: 8px solid #ddd;\n    }\n    .sub-menu {\n      height: 40px;\n      line-height: 40px;\n      position: relative;\n      &:not(:last-child):after {\n        /*position: absolute;*/\n        content: \"\";\n        width: 50%;\n        height: 1px;\n        color: #ff6d6d;\n        bottom: 0;\n        left: 25%;\n        z-index: 99;\n      }\n    }\n  }\n}\n@media (max-width: 960px) {\n  #layout-header {\n    padding: 0 20px;\n  }\n}\n@media (max-width: 600px) {\n  #layout-header {\n    padding: 0 10px;\n  }\n  .menus-btn {\n    display: block;\n    visibility: visible;\n  }\n  .site-menus {\n    position: absolute;\n    display: none;\n    visibility: hidden;\n    background-color: #f9f9f9;\n    width: 100%;\n    left: 0;\n    top: 80px;\n    z-index: -9;\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\n    .menu-item {\n      position: relative;\n      height: unset;\n      &:not(:last-child) {\n        margin-right: 0;\n      }\n    }\n    .childMenu {\n      position: relative;\n      width: 100%;\n      top: 0;\n      background-color: #f3f3f3;\n      opacity: 1;\n      visibility: visible;\n      border: none;\n      box-shadow: none;\n      &:before,\n      &:after {\n        content: \"\";\n        position: relative;\n        width: 0;\n        height: 0;\n        border-left: 0;\n        border-right: 0;\n        border-bottom: 0;\n      }\n    }\n  }\n  .site-menus.mobileShow {\n    display: inline-block;\n    visibility: visible;\n    z-index: 99;\n  }\n}\n</style>\n"],"sourceRoot":"src/components/layout"}]}